      <vraagdata vraagnummer="103" onderwerp="de breedte van de nieuwe dubbeldeks– treinen van de Nederlandse Spoor– wegen." indiendatum="1991-10-7">
         <vrager partij="VVD" oorsprong="parlement">Jorritsma-Lebbink</vrager>
         <vraagt-aan/>
      </vraagdata>
      <antwoorddata antwoorddatum="1991-10-28">
         <antwoorder functie="minister" ministerie="Verkeer en Water– staat">Maij-Weggen</antwoorder>
         <mede-antwoorder/>
      </antwoorddata>
