      <vraagdata vraagnummer="7" onderwerp="de zeedijkversterkingen." indiendatum="6 mei 1986)">
         <vrager partij="CDA" oorsprong="parlement">Eversdijk</vrager>
         <vraagt-aan/>
      </vraagdata>
      <antwoorddata antwoorddatum="9 juni 1986)">
         <antwoorder functie="minister" ministerie="Verkeer en Waterstaat">Smit-Kroes</antwoorder>
