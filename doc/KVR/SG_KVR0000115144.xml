      <vraagdata vraagnummer="663" onderwerp="de organisatie van het ambulan- cevervoer." indiendatum="6 maart 1986)">
         <vrager partij="VVD" oorsprong="parlement">Franssen</vrager>
         <vraagt-aan/>
      </vraagdata>
      <antwoorddata antwoorddatum="28 april 1986)">
         <antwoorder functie="staatssecretaris" ministerie="Welzijn, Volksgezond- heid en Cultuur">Van der Heijden</antwoorder>
         <mede-antwoorder/>
      </antwoorddata>
